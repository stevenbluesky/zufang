.jconfirm.jconfirm-center .jconfirm-bg {
  background-color: #444;
  opacity: .2;
}
.jconfirm.jconfirm-center .jconfirm-box {
  box-shadow: 0 2px 6px rgba(0, 0, 0, 0.2);
  border-radius: 5px;
}
.jconfirm.jconfirm-center .jconfirm-box.jconfirm-type-blue {
  border: none;
}
.jconfirm.jconfirm-center .jconfirm-box.jconfirm-type-green {
  border: none;
}
.jconfirm.jconfirm-center .jconfirm-box.jconfirm-type-red {
  border: none;
}
.jconfirm.jconfirm-center .jconfirm-box.jconfirm-type-orange {
  border: none;
}
.jconfirm.jconfirm-center .jconfirm-box.jconfirm-type-purple {
  border: none;
}
.jconfirm.jconfirm-center .jconfirm-box.jconfirm-type-dark {
  border: none;
}
.jconfirm.jconfirm-center .jconfirm-box div.jconfirm-title-c {
  text-align: center;
  font-size: 28px;
  font-weight: normal;
}
.jconfirm.jconfirm-center .jconfirm-box div.jconfirm-title-c > * {
  padding-bottom: 25px;
}
.jconfirm.jconfirm-center .jconfirm-box div.jconfirm-title-c .jconfirm-icon-c {
  margin-right: 8px;
  margin-left: 0px;
}
.jconfirm.jconfirm-center .jconfirm-box div.jconfirm-content-pane {
  margin-bottom: 25px;
}
.jconfirm.jconfirm-center .jconfirm-box div.jconfirm-content {
  text-align: center;
}
.jconfirm.jconfirm-center .jconfirm-box .jconfirm-buttons {
  text-align: center;
}
.jconfirm.jconfirm-center .jconfirm-box .jconfirm-buttons button {
  text-transform: uppercase;
  font-size: 14px;
  font-weight: bold;
  text-shadow: none;
}
.jconfirm.jconfirm-center.jconfirm-rtl .jconfirm-box div.jconfirm-title-c .jconfirm-icon-c {
  margin-left: 8px;
  margin-right: 0px;
}